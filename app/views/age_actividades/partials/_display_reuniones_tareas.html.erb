<p class="text-info">
	<small>
		<b><%= actividad %></b>
		<%= link_to "/age_actividades/new?t=#{actividad[0]}&class_name=#{objeto.class.name}&objeto_id=#{objeto.id}" do %>
			<i class="bi bi-plus-circle"></i>
		<% end %>
	</small>
</p>


<% unless coleccion.empty?  %>
	<% coleccion.each do |act| %>
		<%= render partial: get_partial(act.class.name.tableize, 'list', "lobjeto") , locals: {objeto: act} %></br>
	<% end %>
<% end %>