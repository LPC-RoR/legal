<% if objeto.krn_investigadores.first.present? %>
	<%= render partial: 'rprts/shrd/tr', locals: {glosa: "Investigador asignado a la denuncia:", valor: objeto.krn_investigadores.first.krn_investigador} %>
<% end %>
<% if objeto.objcn_invstgdr? %>
	<%= render partial: 'rprts/shrd/tr', locals: {glosa: "Se presento objeción de imparcialidad al investigador asignado.", pddng: 1} %>
<% end %>

<% if @objeto.objcn_acogida %>
	<% if objeto.krn_investigadores.second.present? %>
		<%= render partial: 'rprts/shrd/tr', locals: {glosa: "Segundo investigador asignado a la denuncia:", valor: objeto.krn_investigadores.second.krn_investigador} %>
	<% else %>
		<%= render partial: 'rprts/shrd/tr', locals: {glosa: "Objeción de imparcialidad acogida, se debe asignar otro investigador.", pddng: 1} %>
	<% end %>
<% elsif @objeto.objcn_rechazada %>
	<%= render partial: 'rprts/shrd/tr', locals: {glosa: "Objeción de imparcialidad rechazada, se confirma investigador asignado.", pddng: 1} %>
<% end %>