<div class="rprt_bx" style="line-height: 1.6;">
	<div class="rprt_bx_ttl" style="font-size: 1rem;">Documentos y datos de los participantes</div>
	<div class="mt-2 ms-3">

		<% objeto.krn_denunciantes.each do |dnncnt| %>
			<%= render partial: 'rprts/krn_reportes/rprt_dnnc/stts', locals: {objt: dnncnt, acts: acts} %>
			<div class="mt-2">
				<% dnncnt.krn_testigos.each do |tstg| %>
					<%= render partial: 'rprts/krn_reportes/rprt_dnnc/stts', locals: {objt: tstg, acts: acts} %>
				<% end %>
			</div>
		<% end %>

		<!-- DENUNCIANTES OK Y MOTIVO DISTINTO DE VIOLENCIA -->
		<% if objeto.violencia? %>
			<%= render partial: 'cuentas/prtls/inline_mssg', locals: {txt: 'Denuncia de violencia en el trabajo ejercida por terceros. Las personas denunciadas (si las hay) se detallan en las declaraciones.'} %>
		<% else %>

			<% objeto.krn_denunciados.each do |dnncd| %>
				<%= render partial: 'rprts/krn_reportes/rprt_dnnc/stts', locals: {objt: dnncd, acts: acts} %>
				<div class="mt-2">
					<% dnncd.krn_testigos.each do |tstg| %>
						<%= render partial: 'rprts/krn_reportes/rprt_dnnc/stts', locals: {objt: tstg, acts: acts} %>
					<% end %>
				</div>
			<% end %>

		<% end %>

	</div>
</div>