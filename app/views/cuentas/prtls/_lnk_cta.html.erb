<%= render layout: '0p/tltp_lnk', locals: {lnk: "/cuentas/#{objeto.class.name.downcase[0]}_#{objeto.id}/dnncs", tltp: 'Acceder a la cuenta' } do %>
	<%= s_rut(objeto.rut) %> 
	<i class="bi bi-dot"></i>
	<%= objeto.razon_social %>
<% end %>
<% if dog? %>
	<i class="bi bi-dot"></i>
	<%= render layout: '0p/layout/tltp_lnk', locals: {lnk: objeto, tltp: 'Ver productos'} do %>
		<i class="bi bi-box-seam"></i>
	<% end %>
<% end %>