<% typ ||= 'tr' %>

<%= render partial: 'cuentas/indx/emprs_head', locals: {objt: @objeto} %>
<%= render partial: 'cuentas/indx/tab', locals: {objt: @objeto} if current_usuario.admin? %>

<%= render partial: '0p/ttl_clss', locals: {source: source, dir: 'indx', hide_gly: true} %>

<!-- Manejo de mensajes de inicio de cuenta! -->
<!-- EVALUAR Usando tenants hay que cambiar dog? por operacion? -->
<% if (@licencia_activa and current_usuario.admin?) or current_usuario.dog? %>

	<% case action_name %>
	<% when 'usrs' %>
		<%= render partial: 'cuentas/usrs' %>
	<% when 'dnncs' %>
		<%= render partial: '0p/tbl_clss', locals: {clccn: @pgnt[source], dir: 'list', pgnt: true} if @objeto.cnfgrcn_ok? %>
	<% else %>
		<%= render partial: '0p/tbl_clss', locals: {clccn: @pgnt[source], dir: 'list', typ: typ, pgnt: true} %>
	<% end %>

	<% if action_name == 'nmn' %>
		<!-- El ingreso de Contactos habilita opciones de reportes, no inhabilitan el ingreso de una Denuncia -->
		<%= render partial: '0p/ttl_clss', locals: {source: 'app_contactos', dir: 'indx', hide_gly: true} %>
		<%= render partial: '0p/tbl_clss', locals: {clccn: @cntcts, dir: 'list'} %>
	<% end %>

<% end %>