<div class="pt-2 text-center text-primary">
	<h5><i class="bi bi-lightbulb"></i></h5>
</div>
<!-- No se incluyó @proc_objt porque el tamaño es demasiado para un modal -->
<% [@etp_last].each do |objt| %>
	<% unless objt.blank? %>
		<ul>
		<% objt.lgl_temas.ordr.each do |tm| %>
			<li><%= render partial: '0p/mdl/lnk', locals: {txt: tm.lgl_tema, cdg: tm.codigo} %></li>
			<%= render partial: '0p/mdl/mdl', locals: {sz: 'lg', scrll: true, cdg: tm.codigo, ttl: tm.lgl_tema, prtl: prtl_name(@lgl_temas[tm.codigo], 'frame', 'frame'), lcls: {objeto: @lgl_temas[tm.codigo]}} %>
		<% end %>
		</ul>
	<% end %>
<% end %>