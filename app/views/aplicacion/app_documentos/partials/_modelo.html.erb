<tr class="table-<%=table_row_type(objeto)%>">

	<td>
		<i class="bi bi-file-text"></i>
		<small> <%= l_fecha(objeto.created_at) %> </small>
	</td>

	<td>
		<%= render partial: '0p/general/btn_turbolink', locals: {texto: nil, gly: 'download', link: objeto.archivo.url, delete: false, clr: color(objeto.class.name.tableize)} if objeto.archivo.present? %>
	</td>

	<td><%= render partial: '0p/tabla/btns', locals: {a: 'index', objeto: objeto} %></td>

</tr>