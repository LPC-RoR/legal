<% objeto ||= @objeto %>
<% prompt ||= nil %>
<% id     ||= nil %>
<% tdata   ||= nil %>
<div class="form-floating mb-3" id="<%=id%>">
	<%= form.select token, options_for_select(sel_list, (objeto.blank? ? nil : objeto.send(token.to_s))), { prompt: prompt }, class: 'form-select', data: tdata %>
	<%= form.label token, label, for: token.to_s %>
</div>