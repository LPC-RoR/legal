<% shdw    = shdw ? true : false %>
<% typ  ||= 'tr' %>
<% dir  ||= 'list' %>
<div class="<%='shadow' if shdw%> mt-1">

	<% if typ == 'tr' %>
	<table class="table table-sm table-borderless table-striped table-hover">
		<tbody>
	<% end %>

			<% clccn.each do |objt| %>
				<% if prtl?(objt, 'list', "objeto") %>
					<%= render partial: objt_prtl_name(objt) , locals: {objeto: objt} %>
				<% elsif typ == 'frame' %>
					<%= render partial: '0p/objt/objt_frame', locals: {objt: objt} %>
				<% else %>

					<% if prtl?(objt, 'list', typ) %>
						<%= render partial: prtl_name(objt, 'list', typ), locals: {objt: objt} %>
					<% else %>
						<%= render partial: "0p/tbl/#{typ}", locals: {objt: objt, ordr: ordr, crud: crud} %>
					<% end %>

				<% end %>
			<% end %>

	<% if typ == 'tr' %>
		</tbody>
	</table>
	<% end %>
</div>