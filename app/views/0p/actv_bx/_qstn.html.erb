<div class="qstn_bx">
	<div class="qstn">
		<% if prtl?(c, 'code/qstn', code) %>
			<%= render partial: prtl_name(c, 'code/qstn', code), locals: { objeto: objeto} %>
		<% else %>
			Falta código de la pregunta de <%= code %>
			<%= actvbx_q_txt[code] %>
		<% end %>

	</div>
	<div class="optns">
		<div class="typ">
			<i class="bi bi-<%=actvbx_gly[accn]%>"></i> 
			<%= accn %>
		</div>
		<div class="lnks">
			<% if prtl?(c, 'code/lnks', code) %>
				<%= render partial: prtl_name(c, 'code/lnks', code), locals: { objeto: objeto} %>
			<% else %>
				Falta código del link de <%= code %>
				<%= render partial: prtl_name(c, 'qstns', "lnk_#{code}"), locals: {objeto: objeto} %>
			<% end %>

		</div>
	</div>
</div>