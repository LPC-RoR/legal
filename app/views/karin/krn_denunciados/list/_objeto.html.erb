<%= render partial: '0p/general/head_objt', locals: {clss: 'prtcpnts_objt' ,objeto: objeto, ownr: @objeto} %>

<% if objeto.empleado_externo and (not objeto.emprs_extrn_prsnt?) %>
	<div class="dlg_bsc">
		<i class="bi bi-bug"></i>
		Empleado externo, se debe ingresar su empleador.
	</div>
<% end %>

<% if objeto.articulo_516 and (not objeto.direccion_notificacion.present?) %>
	<div class="dlg_bsc">
		<i class="bi bi-bug"></i>
		Empleado no tiene acceso a correo electrónico, se debe ingresar su dirección de notificación.
	</div>
<% end %>

<% if (not objeto.rut.present?) %>
	<div class="dlg_bsc">
		<i class="bi bi-bug"></i>
		Se debe completar el la cédula de identidad del denunciado.
	</div>
<% end %>

<div class="dsply">
	<%= render partial: prtl_name('krn_denunciados', 'list', 'cllps'), locals: {objeto: objeto} %>
</div>

