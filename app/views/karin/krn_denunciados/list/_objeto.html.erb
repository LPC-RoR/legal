	<%#= render partial: prtl_name('ctr_etapas', 'code', 'objeto'), locals: {objeto: etp, ownr: objeto, scndry: 'KrnDenunciado'} if prtl?('ctr_etapas', 'code', 'objeto') %>
<%# end %>