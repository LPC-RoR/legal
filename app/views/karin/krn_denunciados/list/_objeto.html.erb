<%= render partial: '0p/general/head_objt', locals: {clss: 'prtcpnts_objt' ,objeto: objeto, ownr: @objeto} %>

<% if objeto.empleado_externo and (not objeto.emprs_extrn_prsnt?) %>
	<div class="dlg_bsc">
		<i class="bi bi-bug"></i>
		La persona denunciada depende de otro empleador, el que debe ser identificado.
	</div>
<% end %>

<% if objeto.articulo_516 and (not objeto.direccion_notificacion.present?) %>
	<div class="dlg_bsc">
		<i class="bi bi-bug"></i>
		La persona denunciante no tiene acceso a correo electrónico, se debe registrar su dirección de notificación.
	</div>
<% end %>

<% if (not objeto.rut.present?) %>
	<div class="dlg_bsc">
		<i class="bi bi-bug"></i>
		Se debe registrar la cédula de identidad del denunciado.
	</div>
<% end %>

<div class="dsply">
	<%= render partial: prtl_name('krn_denunciados', 'list', 'cllps'), locals: {objeto: objeto} %>
</div>