		<%= render partial: 'cuentas/prtls/proceso/hnt', locals: {txt: 'Registre la fecha en la que se envió el informe de investigación a la Dirección del Trabajo.'} %>
		<%= render partial: '0p/proc/frm', locals: {tar: @tar_last, objeto: @objeto, code: 'fecha_env_infrm', plz_fld: true} %>
	<% end %>
<% end %>