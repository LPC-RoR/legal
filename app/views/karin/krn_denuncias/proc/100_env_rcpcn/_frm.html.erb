<%= render partial: prtl_name(@objeto, 'shw/proceso', 'drvcns_invstgdrs') %>

<%= render layout: 'cuentas/prtls/proceso/actn' do %>
	<% if @objt['on_dt?'] %>
		<%= render partial: 'cuentas/prtls/proceso/hnt', locals: {txt: 'Registre la fecha en la que se recibió el informe de investigación desde la Dirección del Trabajo.'} %>
		<%= render partial: '0p/proc/frm', locals: {tar: @tar_last, objeto: @objeto, code: 'fecha_rcpcn_infrm', plz_fld: true} %>
