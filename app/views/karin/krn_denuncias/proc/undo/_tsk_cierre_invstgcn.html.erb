				<%= render partial: '0p/niler', locals: {objeto: @objeto, tkn: :fecha_hora_corregida, prmpt: 'Plazo otorgado para entregar la denuncia corregida.'} %>
			</div>
		<% else %>
			<% if @objeto.evlcn_incnsstnt %>
				<%= render layout: 'cuentas/prtls/proceso/wrp_lnk_proceso' do %>
					<%= render partial: '0p/swtch', locals: {objeto: @objeto, tkn: :evlcn_incnsstnt, prmpt: 'La denuncia presenta inconsistencias, debe ser devuelta a la persona denunciante para su correcciÃ³n.', stts: ['denuncia sin inconsistencias', 'Denuncia con inconsistencias']} %>
				<% end %>
			<% end %>

			<% if @objeto.evlcn_ok %>
				<%= render layout: 'cuentas/prtls/proceso/wrp_lnk_proceso' do %>
					<%= render partial: '0p/swtch', locals: {objeto: @objeto, tkn: :evlcn_ok, prmpt: 'La denuncia no presenta inconsistencias.', stts: ['denuncia sin inconsistencias', 'denuncia que debe ser analizada']} %>
				<% end %>
			<% end %>
		<% end %>


	</div>
</div>