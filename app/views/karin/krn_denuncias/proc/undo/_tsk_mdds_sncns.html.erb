<div class="border rounded-2 my-1">
	<div class="p-2 bg_azul_claro" style="font-size: 1rem;">
		<i class="bi bi-cone-striped text-danger"></i> Deshacer ultimas acciones:
	</div>
	<div>
		<% if @objeto.on_dt? %>
			<%#= render layout: 'cuentas/prtls/proceso/wrp_lnk_proceso' do %>
			<%#= render partial: '0p/niler', locals: {objeto: @objeto, tkn: :fecha_rcpcn_infrm, prmpt: 'Fecha en la que se recibe el informe de investigación desde la Dirección del Trabajo.'} %>
			<%# end %>
		<% else %>
			<% if @objeto.fecha_prnncmnt? %>
			<%= render layout: 'cuentas/prtls/proceso/wrp_lnk_proceso' do %>
				<%= render partial: '0p/niler', locals: {objeto: @objeto, tkn: :fecha_prnncmnt, prmpt: 'Fecha en la que la Dirección del Trabajo se pronunció acerca del informe de investigación.'} %>
			<% end %>
			<% end %>

			<% if @objeto.prnncmnt_vncd? %>
			<%= render layout: 'cuentas/prtls/proceso/wrp_lnk_proceso' do %>
				<%= render partial: '0p/swtch', locals: {objeto: @objeto, tkn: :prnncmnt_vncd, prmpt: 'El plazo para recibir el pronunciamiento venció.', glys: ['arrow-counterclockwise', 'cursor'], stts: ['plazo no vencido', 'plazo vencido']} %>
			<% end %>
			<% end %>
		<% end %>
	</div>
</div>