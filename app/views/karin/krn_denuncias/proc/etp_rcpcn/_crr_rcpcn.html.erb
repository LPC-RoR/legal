<% if @objt['inf_cierre?'] %>
	<%= render layout: 'cuentas/prtls/proceso/actn' do %>
		<div class="border rounded-2">

			<% if @objeto.rcp_dt? %>
				<%= render partial: '0p/proc/etp_frm', locals: {etp: @etp_last, objeto: @objeto, code: 'fecha_ntfccn'} %>
			<% end %>

			<% if (@objeto.dnnc.investigacion_local or @objeto.investigacion_externa) %>
				<%= render partial: '0p/proc/etp_frm', locals: {etp: @etp_last, objeto: @objeto, code: 'fecha_trmtcn', plz_fld: true} %>
			<% end %>

			<% if @objeto.krn_derivaciones.on_dt? %>
				<%= render partial: '0p/proc/etp_frm', locals: {etp: @etp_last, objeto: @objeto, code: 'fecha_hora_dt'} %>
			<% end %>

		</div>
	<% end %>
<% end %>