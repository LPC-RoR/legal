<%= render layout: 'cuentas/prtls/proceso/actn' do %>
	<% if @objeto.fecha_prnncmnt? %>
		<%= render layout: 'cuentas/prtls/proceso/lnk', locals: {fw: 'bold'} do %>
			<%= render partial: '0p/niler', locals: {objeto: @objeto, tkn: :fecha_prnncmnt, prmpt: 'Fecha en la que se recibió el pronunciamiento de la Dirección del Trabajo.'} %>
		<% end %>
	<% end %>
	<% if @objeto.prnncmnt_vncd? %>
		<%= render layout: 'cuentas/prtls/proceso/lnk', locals: {fw: 'bold'} do %>
			<%= render partial: '0p/swtch', locals: {objeto: @objeto, tkn: :prnncmnt_vncd, prmpt: 'El plazo para recibir el pronunciamiento venció.', glys: ['arrow-counterclockwise', 'cursor'], stts: ['plazo no vencido', 'plazo vencido']} %>
		<% end %>
	<% end %>
<% end %>