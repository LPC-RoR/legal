<% unless @proc[:etp_rcpcn][:fls_mss].empty? %>
	<% @proc[:etp_rcpcn][:fls_mss].each do |fl_desc| %>
		<%= render partial: '0p/ntc', locals: {color: 'danger', ntc: "Falta el archivo #{fl_desc}"} %>
	<% end %>
<% else %>
	<%= render partial: '0p/ntc', locals: {color: 'primary', ntc: 'Todos los archivos requeridos ingresados.'} %>
<% end %>