<% unless scp_activo? %>
	<%= render layout: '0p/layout/tltp_lnk', locals: {lnk: "/cuentas/#{objeto.ownr.class.name.downcase[0]}_#{objeto.ownr.id}/dnncs", tltp: objeto.ownr.razon_social} do %>
		<i class="bi bi-inboxes"></i>
	<% end %>

	<% if dog? %>
		<%= render layout: '0p/tltp_lnk', locals: {lnk: "/#{cntrllr(objeto)}/#{objeto.id}/prg", tltp: 'Purge'} do %>
			<i class="bi bi-trash"></i>
		<% end %>
	<% end %>
<% end %>