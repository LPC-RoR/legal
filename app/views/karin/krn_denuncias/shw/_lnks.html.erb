<% unless scp_activo? %>

	<%= render partial: '0p/tbl/lnk_new', locals: {source: 'notas', ownr: objeto, gly: 'chat-left-text'} %>

	<i class="bi bi-dot"></i>
	<% if dog? %>
		<%= render layout: '0p/tltp_lnk', locals: {lnk: "/#{cntrllr(objeto)}/#{objeto.id}/prg", tltp: 'Purge'} do %>
			<i class="bi bi-trash"></i>
		<% end %>
	<% end %>

	<%= render layout: '0p/layout/tltp_lnk', locals: {lnk: "/cuentas/#{objeto.ownr.id}/#{objeto.ownr.class.name == 'Empresa' ? 'e' : 'c'}dnncs", tltp: objeto.ownr.razon_social} do %>
		<i class="bi bi-building"></i>
	<% end %>
<% end %>