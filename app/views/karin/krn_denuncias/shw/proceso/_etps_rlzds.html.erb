<% unless @kproc.etapa == :etp_rcpcn %>

	<%= render layout: 'karin/krn_denuncias/shw/proceso/tsk_tmplt', locals: {ttl: 'Etapas concluidas', gly: 'caret-right-fill'} do %>
		<table class="table table-sm table-borderless table-striped table-hover">
			<tbody>
				<tr>
					<th>Etapa</th>
					<th>Fecha límite</th>
					<th class="text-end">Fecha término</th>
				</tr>
				<% [:etp_rcpcn, :etp_invstgcn, :etp_infrm, :etp_prnncmnt, :etp_mdds_sncns].each do |etp| %>
					<% unless @kproc.etapa == etp %>
						<% if ClssPrcdmnt.plz_dsply(etp, @objeto) %>
							<%= render partial: 'karin/krn_denuncias/shw/proceso/etp_rlzd_objt', locals: {objeto: @objeto, tkn: etp} %>
						<% end %>
					<% end %>
				<% end %>
			</tbody>
		</table>
	<% end %>

<% end %>