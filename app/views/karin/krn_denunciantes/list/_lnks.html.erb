<% unless objeto.rlzd or objeto.registros? %>
	<% unless (@objeto.fecha_trmtcn? or @objeto.derivaciones?) %>
		<i class="bi bi-dot"></i>
		<%= render partial: '0p/tabla/link_editar', locals: {objeto: objeto} %>
		<% unless @objeto.denunciados? and @objeto.krn_denunciantes.count == 1 %>
			<%= render partial: '0p/tabla/link_eliminar', locals: {objeto: objeto} %>
		<% end %>
	<% end %>
<% end %>