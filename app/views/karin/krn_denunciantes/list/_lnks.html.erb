<% unless objeto.rlzd or objeto.registros? %>
	<% unless (@objeto.rgstrs_ok? and (@objeto.fecha_trmtcn? or @objeto.derivaciones?)) %>
		<%= render partial: '0p/tbl/lnk_editar', locals: {objeto: objeto} %>
