<p id="notice"><%= notice %></p>

<h1>Age Usuarios</h1>

<table>
  <thead>
    <tr>
      <th>Owner class</th>
      <th>Owner</th>
      <th>Age usuario</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @age_usuarios.each do |age_usuario| %>
      <tr>
        <td><%= age_usuario.owner_class %></td>
        <td><%= age_usuario.owner_id %></td>
        <td><%= age_usuario.age_usuario %></td>
        <td><%= link_to 'Show', age_usuario %></td>
        <td><%= link_to 'Edit', edit_age_usuario_path(age_usuario) %></td>
        <td><%= link_to 'Destroy', age_usuario, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Age Usuario', new_age_usuario_path %>
