<div class="row">
	<div class="col-9">
		<small><small>
			<% if objeto.estado == 'realizado' %>
				<i class="bi bi-check-lg text-primary"></i>
			<% else %>
				<i class="bi bi-circle-fill text-<%=objeto.text_color%>"></i> 
			<% end %>
			<b class="text-<%= objeto.text_color %>">
				<%= dma_hm(objeto.created_at) %>
			</b>
		</small></small>
	</div>
	<div class="col-3" align="right">
		<small><small>
			<% unless objeto.estado == 'realizado' %>
				<%= render partial: '0p/tabla/links_e', locals: {objeto: objeto} %>
			<% end %>
		</small></small>
	</div>
</div>