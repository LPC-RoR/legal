<div class="ac_bjt">

	<%= render partial: 'actividades/shr/ctvdd/fecha', locals: {objeto: objeto, usuarios: usuarios} %>

	<%= render partial: 'actividades/shr/ctvdd/ctvdd', locals: {objeto: objeto} %>

	<%#= render partial: 'actividades/age_actividades/list/elems/cllp_display_encargados', locals: {objeto: objeto} %>


	<% unless objeto.age_logs.empty? %>
		<% objeto.age_logs.order(:created_at).each do |log| %>
			<%= render partial: '0p/nota/nota', locals: {nota: "Cambio de fecha desde el #{dma(log.fecha)}", clr: 'g', objeto: nil} %>
		<% end %>
	<% end %>

	<% unless objeto.privada %>
		<% if objeto.age_usuarios.empty? %>
			<%= render partial: '0p/nota/nota', locals: {nota: "Actividad sin encargados", clr: 'y', objeto: nil} %>
		<% end %>

		<% if usuarios.empty? %>
			<%= render partial: '0p/nota/nota', locals: {nota: "No hay usuarios disponibles", clr: 'y', objeto: nil} %>
		<% end %>
	<% end %>

	<% unless objeto.age_antecedentes.blank? %>
		<% objeto.age_antecedentes.each do |ntcdt| %>
			<%= render partial: '0p/nota/nota', locals: {nota: ntcdt.nota, clr: ntcdt.color, objeto: ntcdt} %>
		<% end %>
	<% end %>
</div>