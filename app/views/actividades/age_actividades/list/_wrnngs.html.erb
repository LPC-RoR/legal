<div class="wrnngs">
	<% unless objeto.age_logs.empty? %>
		<% objeto.age_logs.order(:created_at).each do |log| %>
			<%= render partial: '0p/nota/wrnng', locals: {wrnng: "Cambio de fecha desde el #{dma(log.fecha)}" } %>
		<% end %>
	<% end %>

	<% unless objeto.privada %>
		<% if objeto.age_usuarios.empty? %>
			<%= render partial: '0p/nota/wrnng', locals: {wrnng: "Actividad sin encargados" } %>
		<% end %>

		<% if usuarios.empty? %>
			<%= render partial: '0p/nota/wrnng', locals: {wrnng: "No hay usuarios disponibles" } %>
		<% end %>
	<% end %>
</div>