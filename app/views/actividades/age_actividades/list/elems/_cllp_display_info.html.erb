<%= render layout: '0p/collapse/display', locals: {label: 'Info', objeto: objeto} do %>
	<hr>
	<small><small class="text-muted">
		Creado por <b><%= objeto.nombre_creador %></b>
	</small></small>
	<hr>

	<%= render partial: 'actividades/age_actividades/list/elems/display_antecedentes', locals: {objeto: objeto} %>

	<% unless objeto.age_logs.empty? %>
		<hr>
		<% objeto.age_logs.order(created_at: :desc).each do |log| %>
			<small><small><%= dma_hm(log.fecha) %> : <%=log.actividad%></small></small></br>
		<% end %>
	<% end %>

<% end %>