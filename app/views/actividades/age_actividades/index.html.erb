<div class="f_50lft_col">
	<div class="lft">
		<%= render partial: 'actividades/shr/semana_nav' %>
		<% @week.each do |dt_fecha| %>
			<%= render partial: 'actividades/shr/bjt_dia', locals: {objeto: dt_fecha, usuarios: @age_usuarios} %>
		<% end %>
	</div>
	<div class="rght">
		<% unless @usuario.blank? %>
		    <%= render partial: '0p/general/clss_dir_c_ttl', locals: {clss: 'shw_ttl', dir: 'indx', source: 'age_pendientes', ownr: @usuario, bck: nil} %>

			<%= render layout: '0p/collapse/display', locals: {label: 'Personaliza', objeto: nil} do %>
				<%= render partial: 'actividades/age_actividades/partials/form_nombre' %>
			<% end %>

			<%= render partial: '0p/tabs/tabs', locals: {token: :tab} %>
			<%= render partial: "/actividades/age_pendientes/display", locals: {owner: @usuario} %>
		<% end %>
	</div>
</div>