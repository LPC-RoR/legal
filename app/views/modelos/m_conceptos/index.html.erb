<p id="notice"><%= notice %></p>

<h1>M Conceptos</h1>

<table>
  <thead>
    <tr>
      <th>M concepto</th>
      <th>M modelo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @m_conceptos.each do |m_concepto| %>
      <tr>
        <td><%= m_concepto.m_concepto %></td>
        <td><%= m_concepto.m_modelo_id %></td>
        <td><%= link_to 'Show', m_concepto %></td>
        <td><%= link_to 'Edit', edit_m_concepto_path(m_concepto) %></td>
        <td><%= link_to 'Destroy', m_concepto, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New M Concepto', new_m_concepto_path %>
