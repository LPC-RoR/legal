<small class="text-muted">
<div class="row">
	<div class="col-8">
		<b>
			<%= render layout: '0p/layout/tooltip_link', locals: {link: objeto, tooltip: 'Ver Detalle Cliente' } do %>
				<%= objeto.d_rut %>
			<% end %>
		</b> 
	</div>
	<div class="col-4" align='right'>
		<% unless objeto.archivos_faltantes.empty? and objeto.archivos_vacios.empty? %>
			<i class="bi bi-file-check text-danger"></i>
			<small class='text-danger'><%= objeto.archivos_faltantes.count + objeto.archivos_vacios.count %></small>
		<% end %>
		<% if false %>
			<i class="bi bi-dot"></i>
			<%= render layout: '0p/layout/tooltip_link', locals: {link: "/servicios/organizacion?oid=#{objeto.id}", tooltip: 'Ver OrganizaciÃ³n' } do %>
				<i class="bi bi-diagram-3"></i>
			<% end %>
		<% end %>
		<% if operacion? %>
			<i class="bi bi-dot"></i>
			<%= render partial: '0p/tabla/links_e', locals: {objeto: objeto} %>
		<% end %>
	</div>
</div>
</small>