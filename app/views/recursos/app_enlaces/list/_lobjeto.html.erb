<small>
	<i class="bi bi-dot"></i> 
	<% if objeto.enlace.present? %>
		<%= link_to objeto.enlace, class: 'sb-link', target: :_blank do %>
			<%= objeto.descripcion %> 
		<% end %>
	<% else %>
		<%= objeto.descripcion %> 
	<% end %>
	<%= render partial: '0p/tabla/links_e', locals: {objeto: objeto} %>
</small>
