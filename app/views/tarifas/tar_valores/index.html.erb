<p id="notice"><%= notice %></p>

<h1>Tar Valores</h1>

<table>
  <thead>
    <tr>
      <th>Codigo</th>
      <th>Detalle</th>
      <th>Valor uf</th>
      <th>Valor</th>
      <th>Owner class</th>
      <th>Owner</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tar_valores.each do |tar_valor| %>
      <tr>
        <td><%= tar_valor.codigo %></td>
        <td><%= tar_valor.detalle %></td>
        <td><%= tar_valor.valor_uf %></td>
        <td><%= tar_valor.valor %></td>
        <td><%= tar_valor.owner_class %></td>
        <td><%= tar_valor.owner_id %></td>
        <td><%= link_to 'Show', tar_valor %></td>
        <td><%= link_to 'Edit', edit_tar_valor_path(tar_valor) %></td>
        <td><%= link_to 'Destroy', tar_valor, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tar Valor', new_tar_valor_path %>
