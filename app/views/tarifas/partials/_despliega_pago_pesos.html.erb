<% if objeto.pago_generado(pago).blank? %>
	<% if  pago.moneda == 'Pesos' %>
		<%= s_pesos( calcula2(pago.formula_tarifa, objeto, pago) ) %>
	<% else %>
		<% if objeto.uf_calculo_pago(pago).blank? %>
			UF no encontrada
		<% else %>
			<%= s_pesos( calcula2(pago.formula_tarifa, objeto, pago) * objeto.uf_calculo_pago(pago).valor ) %>
		<% end %>
	<% end %>
<% else %>
	<%= s_pesos(objeto.pago_generado(pago).monto_pesos) %>
<% end %>