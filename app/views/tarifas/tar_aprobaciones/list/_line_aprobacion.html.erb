<small>
<div class="row">
	<div class="col-8">
		<%= link_to dma(objeto.fecha), objeto %>
		</br>
		<small><b class="text-info"><%= objeto.cliente.razon_social %></b></small>
	</div>
	<div class="col-4" align='right'>
		<small>
		<%= link_to "/servicios/aprobacion?indice=#{objeto.id}" do %>
			<i class="bi bi-eye"></i>
		<% end %>
		<%= link_to 'facturar', "/tar_aprobaciones/#{objeto.id}/facturar" if objeto.tar_facturaciones.where(tar_factura_id: nil).any? %>
		<%= render partial: '0p/tabla/lnks_e', locals: {objeto: objeto} %>
		</small>
	</div>
</div>
</small>