<div class="objeto-box">
<small>
	<div class="row">
		<div class="col-10">
			<i class="bi bi-briefcase"></i>
			<b><%= objeto.descripcion %></b></br>
			<small>
			<% unless objeto.detalle.blank? %>
				<%= objeto.detalle %></br>
			<% end %>
			<i class="bi bi-dot"></i> <%= objeto.tipo %> : <%= s_moneda(objeto.moneda, objeto.monto) %></br>
			</small>
		</div>
		<div class="col-2" align='right'>
			<small><%= render partial: '0p/tbl/lnks_e', locals: {objeto: objeto} %></small>
		</div>
	</div>
</small>
</div>