<p id="notice"><%= notice %></p>

<h1>Tar Uf Facturaciones</h1>

<table>
  <thead>
    <tr>
      <th>Owner class</th>
      <th>Owner</th>
      <th>Pago</th>
      <th>Fecha uf</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tar_uf_facturaciones.each do |tar_uf_facturacion| %>
      <tr>
        <td><%= tar_uf_facturacion.owner_class %></td>
        <td><%= tar_uf_facturacion.owner_id %></td>
        <td><%= tar_uf_facturacion.pago %></td>
        <td><%= tar_uf_facturacion.fecha_uf %></td>
        <td><%= link_to 'Show', tar_uf_facturacion %></td>
        <td><%= link_to 'Edit', edit_tar_uf_facturacion_path(tar_uf_facturacion) %></td>
        <td><%= link_to 'Destroy', tar_uf_facturacion, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

