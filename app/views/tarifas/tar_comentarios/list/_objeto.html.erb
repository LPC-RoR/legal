<div class="objt">
	<div class="ln">
		<%= render partial: '0p/general/o_objt', locals: {objeto: objeto} %>
	</div>
	<div class="dtl">
		<% if objeto.formula.present? %>
			<i class="bi bi-question-circle"></i> 
			<%= objeto.formula %>
		<% end %>
		<% if objeto.despliegue.present? %>
			<i class="bi bi-arrow-right"></i> 
			<%= objeto.despliegue %>
		<% end %>
	</div>
</div>
