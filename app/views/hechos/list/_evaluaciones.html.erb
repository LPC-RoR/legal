			<i class="bi bi-circle-fill text-success"></i>
		<% end %>
	<% end %>
	<%# unless objeto.st_preparatoria == 'warning' %>
		<%#= render layout: '0p/layout/tooltip_link', locals: { link: "/hechos/#{objeto.id}/set_evaluacion?c=p&e=warning" } do %>
			<!--i class="bi bi-circle-fill text-warning"></i-->
		<%# end %>
	<%# end %>
	<% unless objeto.st_preparatoria == 'dangter' %>
		<%= render layout: '0p/layout/tooltip_link', locals: { link: "/hechos/#{objeto.id}/set_evaluacion?c=p&e=danger", tooltip: 'hecho controvertido' } do %>
			<i class="bi bi-circle-fill text-danger"></i>
		<% end %>
	<% end %>
	</small>
</div>