<div alig='center'>
	<small class="text-info">Contestación</small></br>
	<small>
	<% unless objeto.st_contestacion.blank? %>
		<%= render layout: '0p/layout/tooltip_link', locals: { link: "/hechos/#{objeto.id}/set_evaluacion?c=c&e=nil", tooltip: 'sin evaluacion' } do %>
			<i class="bi bi-circle-fill text-dark"></i>
		<% end %>
	<% end %>
	<% unless objeto.st_contestacion == 'success' %>
		<%= render layout: '0p/layout/tooltip_link', locals: { link: "/hechos/#{objeto.id}/set_evaluacion?c=c&e=success", tooltip: 'hecho pacífico' } do %>
			<i class="bi bi-circle-fill text-success"></i>
		<% end %>
	<% end %>
	<% unless objeto.st_contestacion == 'warning' %>
		<%= render layout: '0p/layout/tooltip_link', locals: { link: "/hechos/#{objeto.id}/set_evaluacion?c=c&e=warning", tooltip: 'revisar' } do %>
			<i class="bi bi-circle-fill text-warning"></i>
		<% end %>
	<% end %>
	<% unless objeto.st_contestacion == 'danger' %>
		<%= render layout: '0p/layout/tooltip_link', locals: { link: "/hechos/#{objeto.id}/set_evaluacion?c=c&e=danger", tooltip: 'hecho controvertido' }  do %>
			<i class="bi bi-circle-fill text-danger"></i>
		<% end %>
	<% end %>
	</small>
</div>
<div alig='center'>
	<small class="text-info">Preparatoria</small></br>
	<small>
	<% unless objeto.st_preparatoria.blank? %>
