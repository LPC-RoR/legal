<div class="objeto-box">
<div class="ln_app">
	<div class="ln_nm">
		<%= objeto.producto %>
	</div>
	<div class="ln_lnks">
		<% if controller_name == 'clientes' %>
			<%= render layout: '0p/layout/tooltip_link', locals: {link: "/productos/#{objeto.id}/elimina_producto?cid=#{@objeto.id}", tooltip: 'Agrega Producto'} do %>
				<i class="bi bi-trash"></i>
			<% end %>
		<% else %>
			<%= render partial: '0p/tabla/links_e', locals: {objeto: objeto} %>
		<% end %>
	</div>

</div>
</div>