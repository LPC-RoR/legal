<p id="notice"><%= notice %></p>

<h1>Cal Dias</h1>

<table>
  <thead>
    <tr>
      <th>Cal dia</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cal_dias.each do |cal_dia| %>
      <tr>
        <td><%= cal_dia.cal_dia %></td>
        <td><%= link_to 'Show', cal_dia %></td>
        <td><%= link_to 'Edit', edit_cal_dia_path(cal_dia) %></td>
        <td><%= link_to 'Destroy', cal_dia, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Cal Dia', new_cal_dia_path %>
