<p id="notice"><%= notice %></p>

<h1>App Control Documentos</h1>

<table>
  <thead>
    <tr>
      <th>App control documento</th>
      <th>Existencia</th>
      <th>Vencimiento</th>
      <th>Ownr class</th>
      <th>Owner</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @app_control_documentos.each do |app_control_documento| %>
      <tr>
        <td><%= app_control_documento.app_control_documento %></td>
        <td><%= app_control_documento.existencia %></td>
        <td><%= app_control_documento.vencimiento %></td>
        <td><%= app_control_documento.ownr_class %></td>
        <td><%= app_control_documento.owner_id %></td>
        <td><%= link_to 'Show', app_control_documento %></td>
        <td><%= link_to 'Edit', edit_app_control_documento_path(app_control_documento) %></td>
        <td><%= link_to 'Destroy', app_control_documento, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New App Control Documento', new_app_control_documento_path %>
