<% unless objeto.rep_doc_controlados.empty? %>

	<% objeto.rep_doc_controlados.ordr.each do |dc| %>
		<% if dc.multiple == true %>
			<%= render partial: "repositorios/shr/dc_fl_lst", locals: {cntrl: @krn_cntrl[dc.codigo], ownr: ownr, fl: ownr.rep_archivos.where(rep_doc_controlado_id: dc.id), dc: dc} %>
		<% else %>
			<%= render partial: "repositorios/shr/dc_fl", locals: {cntrl: @krn_cntrl[dc.codigo], ownr: ownr, fl: ownr.rep_archivos.find_by(rep_doc_controlado_id: dc.id), dc: dc} %>
		<% end %>
	<% end %>

<% end %>