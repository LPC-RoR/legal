<small class="text-info">
	<small><b>Archivos</b></small></br>
	<div class="row">
  	<% objeto.archivos.order(created_at: :desc).each do |archivo| %>
		<div class="col-10">
  		<i class="bi bi-files"></i> 
  		<%= archivo.app_archivo %> 
		</div>
		<div class="col-2" align="right">
  		<small>
  			<%= render partial: '0p/tabla/links_e', locals: {objeto: archivo} %>
  		</small>
		</div>
  	<% end %>
	</div>
</small>