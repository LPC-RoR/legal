<tr>
	<td><i class="bi bi-journals"></i> <small>
		<% if objeto.d_version.present? %>
			<%= link_to objeto.d_version.send('url'), class: 'sb-link', target: :_blank do %>
				<i class="bi bi-download"></i>
				<%= objeto.app_documento %>
			<% end %>
		<% else %>
			<%= objeto.app_documento %>
		<% end %>

		</small> <% if objeto.documento_control %>
			<i class="bi bi-lock"></i>
		<% end %>
	</td>
	<td><small>
		<% if objeto.d_version.present? %>
			<%= link_to objeto, class: 'sb-link' do %>
				<i class="bi bi-list-task"></i>
			<% end %>
		<% end %>
		<%= link_to "/app_archivos/new?class_name=AppDocumento&objeto_id=#{objeto.id}", class: 'sb-link' do %>
			<%= "Documento vacÃ­o" unless objeto.d_version.present? %>
			<i class="bi bi-file-plus"></i>
		<% end %>
		</small>
	</td>

	<td>
		<%= render partial: '0p/tabla/btn_editar', locals: {objeto: objeto} unless objeto.control? %>
		<% if not has_child?(objeto) %>
			<%= render partial: '0p/tabla/btn_eliminar', locals: {objeto: objeto} if (objeto.archivos.empty? and objeto.escaneos.empty?) %>
		<% end %>
	</td>

</tr>