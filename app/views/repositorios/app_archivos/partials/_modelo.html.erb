<tr class="table-<%=table_row_type(objeto)%>">

	<td>
		<i class="bi bi-file-text"></i>
		<% if objeto.archivo.present? %>
			<%= link_to objeto.archivo.url, class: 'sb-link', target: :_blank do %>
				<small>
				<i class="bi bi-download"></i>
				<%= objeto.app_archivo %> </small>
			<% end %>
		<% else %>
			<small><%= objeto.app_archivo %> </small>
		<% end %>
	</td>

	<td>
		<small> <%= dma(objeto.created_at) %> </small>
	</td>

	<td>
		<%= render partial: '0p/tabla/btn_editar', locals: {objeto: objeto} %>
		<% if not has_child?(objeto) %>
			<%= render partial: '0p/tabla/btn_eliminar', locals: {objeto: objeto} %>
		<% end %>
	</td>

</tr>