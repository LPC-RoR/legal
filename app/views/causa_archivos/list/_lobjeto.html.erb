<div class="col-2">
	<small class="text-muted">
		<% if objeto.causa.archivos_registrados %>
			<b><%= objeto.orden %></b>
		<% else %>
			<%= render partial: '0p/tabla/btns_orden', locals: {objeto: objeto} %>
		<% end %>
	</small>
</div>
<div class="col-8">
	<small><small><i class="bi bi-circle-fill text-<%=objeto.text_color%>"></i></small></small> 
	<small class="text-muted">
		<%= render partial: 'repositorios/app_archivos/list/obj_info', locals: {objeto: objeto.app_archivo} %>
		<%= objeto.seleccionado %>
	</small>
</div>
<div class="col-2" align='right'>
	<%= render partial: "causa_archivos/list/set_seleccionado", locals: {objeto: objeto} %>
	<% unless objeto.causa.archivos_registrados %>
		<i class="bi bi-dot"></i>
		<%= render partial: "causa_archivos/list/obj_e", locals: {objeto: objeto} %>
	<% end %>
</div>