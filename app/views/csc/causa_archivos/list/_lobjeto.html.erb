<div class="innr_rchv">

	<div class="ttl_ttl">
		<small><i class="bi bi-circle-fill text-<%=objeto.text_color%>"></i></small>
		<%= render partial: 'repositorios/app_archivos/list/obj_info', locals: {objeto: objeto.app_archivo} %>
	</div>
	<div class="ttl_lnks">
		<% if objeto.causa.archivos_registrados %>
			<b><%= objeto.orden %></b>
		<% else %>
			<%= render partial: '0p/tabla/links_orden', locals: {objeto: objeto} %>
		<% end %>

		<%= render partial: "csc/causa_archivos/list/set_seleccionado", locals: {objeto: objeto} %>
		<% unless objeto.causa.archivos_registrados %>
			<i class="bi bi-dot"></i>
			<%= render partial: "csc/causa_archivos/list/obj_e", locals: {objeto: objeto} %>
		<% end %>
	</div>

</div>