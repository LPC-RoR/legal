	<%= s_rut(objeto.rut) %> 
	<i class="bi bi-dot"></i>
	<%= objeto.razon_social %>
<% end %>
<%= render layout: '0p/layout/tltp_lnk', locals: {lnk: objeto, tltp: 'Ver productos'} do %>
<% end %>
<% if  objeto.principal_usuaria %>
	<i class="bi bi-dot"></i>
	<i class="bi bi-diagram-3"></i>
<% end %>
<% if objeto.contacto %>
	<i class="bi bi-info-square"></i>
<% end %>