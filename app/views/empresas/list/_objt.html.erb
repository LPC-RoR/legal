<%= render partial: 'cuentas/prtls/lnk_cta', locals: {objeto: objeto} %>

<% if  objeto.principal_usuaria %>
	<i class="bi bi-dot"></i>
	<i class="bi bi-diagram-3"></i>
<% end %>
<% if objeto.contacto %>
	<i class="bi bi-info-square"></i>
<% end %>
<% if dog? %>
	<%= render layout: '0p/tltp_lnk', locals: {lnk: "/empresas/#{objeto.id}/prg", tltp: 'Purgar'} do %>
		<i class="bi bi-trash"></i>
	<% end %>
<% end %>