<%= render partial: '0capitan/general/app_ttl', locals: {c: 'tar_valor_cuantias', title: "Cuantía", link: "/tar_valor_cuantias/new?class_name=#{objeto.class.name}&objeto_id=#{objeto.id}", tooltip: 'Agregar item de cuantía' } %>

<% if dog? %>
<%= link_to "/causas/#{objeto.id}/cuantia_to_xlsx" do %>
	To excel
<% end %>
<% end %>

<% unless objeto.valores_cuantia.empty? %>
	<div class="app_box">
		<%= render partial: 'causas/shr/detalle_cuantia', locals: {objeto: objeto, porcentaje_cuantia: false, valores_cuantia: valores_cuantia} %>
	</div>
<% end %>