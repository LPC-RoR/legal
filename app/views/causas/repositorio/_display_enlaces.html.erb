<p class="text-info">
	<small>
		Enlaces 

		<%= link_to "/app_enlaces/new?class_name=#{objeto.class.name}&objeto_id=#{objeto.id}" do %>
			<i class="bi bi-plus-circle"></i>
		<% end %>
	</small>
</p>

<% unless @coleccion['app_enlaces'].empty? %>
	<% @coleccion['app_enlaces'].each do |enlace| %>
		<%= render partial: get_partial(enlace.class.name.tableize, 'list', "lobjeto") , locals: {objeto: enlace} %></br>
	<% end %>
<% end %>